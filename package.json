{
  "name": "my-project",
  "version": "2.0.0",
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn check-all"
    }
  },
  "scripts": {
    "clean": "rm -rf .next;rm .env.local;rm public/*.js",
    "codegen": "graphql-let",
    "dev": "yarn codegen && NODE_ENV=test next",
    "build": "yarn codegen && next build",
    "start": "next start",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --check --write .",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "test": "yarn codegen && jest --update-snapshot --detectOpenHandles",
    "coverage": "yarn codegen && jest --collect-coverage",
    "test-all": "yarn lint && yarn type-check && yarn test",
    "analyzer": "ANALYZE=true NODE_ENV=test next build && yarn start",
    "development": "cp .env.development .env.local && NODE_ENV=development yarn build && yarn start",
    "production": "cp .env.production .env.local && NODE_ENV=production yarn build && yarn start"
  },
  "dependencies": {
    "@apollo/client": "3.1.3",
    "@graphql-tools/load-files": "6.0.18",
    "@graphql-tools/merge": "6.0.18",
    "@graphql-tools/schema": "6.0.18",
    "apollo-server-micro": "2.16.1",
    "graphql": "15.3.0",
    "next": "latest",
    "nprogress": "0.2.0",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.36",
    "@fortawesome/free-brands-svg-icons": "5.15.4",
    "@fortawesome/free-regular-svg-icons": "5.15.4",
    "@fortawesome/free-solid-svg-icons": "5.15.4",
    "@graphql-codegen/cli": "1.17.8",
    "@graphql-codegen/import-types-preset": "1.18.2",
    "@graphql-codegen/plugin-helpers": "1.17.8",
    "@graphql-codegen/typescript": "1.17.8",
    "@graphql-codegen/typescript-operations": "1.17.8",
    "@graphql-codegen/typescript-react-apollo": "2.0.6",
    "@graphql-codegen/typescript-resolvers": "1.17.8",
    "@next/bundle-analyzer": "12.0.6",
    "@testing-library/react": "12.1.2",
    "@types/jest": "27.0.3",
    "@types/node": "16.11.11",
    "@types/nprogress": "0.2.0",
    "@types/react": "17.0.37",
    "@types/react-dom": "16.9.8",
    "@types/react-test-renderer": "17.0.1",
    "@typescript-eslint/eslint-plugin": "5.5.0",
    "@typescript-eslint/parser": "5.5.0",
    "ansi-regex": "5.0.1",
    "autoprefixer": "10.4.0",
    "babel-jest": "27.4.2",
    "eslint": "8.4.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.27.1",
    "eslint-plugin-react-hooks": "4.3.0",
    "eslint-plugin-sonarjs": "0.11.0",
    "graphql-let": "0.x",
    "graphql-tag": "2.11.0",
    "husky": "7.0.4",
    "identity-obj-proxy": "3.0.0",
    "jest": "27.4.3",
    "jest-watch-typeahead": "1.0.0",
    "lint-staged": "12.1.2",
    "next-pwa": "5.4.4",
    "postcss": "8.4.4",
    "prettier": "2.5.1",
    "react-test-renderer": "17.0.1",
    "tailwindcss": "2.2.19",
    "typescript": "4.5.2",
    "yaml-loader": "0.6.0"
  }
}
