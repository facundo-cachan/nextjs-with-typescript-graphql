{
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": true,
      "privateMethod": false,
      "functionBind": false,
      "exportDefaultFrom": false,
      "exportNamespaceFrom": false,
      "decorators": false,
      "decoratorsBeforeExport": false,
      "topLevelAwait": false,
      "importMeta": false
    },
    "transform": {
      "legacyDecorator": true,
      "decoratorMetadata": true,
      "optimizer": {
        "simplify": true,
        "globals": {
          "vars": {
            "__DEBUG__": "true"
          }
        }
      }
    },
    "minify": {
      "compress": {
        "unused": true,
        "drop_console": true
      },
      "mangle": true
    },
  }
}
